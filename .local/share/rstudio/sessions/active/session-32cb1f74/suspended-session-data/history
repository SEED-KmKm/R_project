pacman::p_load(tidyverse, readxl, xtable, ivreg, gmm, modelsummary)
# import data
ajr_dt <- read_excel("AJR2001.xlsx")
setwd("~/econometrics/HW3")
# import data
ajr_dt <- read_excel("AJR2001.xlsx")
# convert data format
ajr_dt$logmrtl = as.numeric(ajr_dt$logmort0)
